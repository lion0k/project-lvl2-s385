{
    "name": "li0n0k/gendiff",
    "description": "cli-program analysis file diff",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexey Abramov",
            "email": "li0n0km0nk@gmail.com"
        }
    ],
    "bin": [
        "bin/gendiff"
    ],
    "scripts": {
        "phpcs": "phpcs",
        "phpcbf": "phpcbf",
        "phpunit": "phpunit",
        "test": "phpunit tests"
    },
    "autoload": {
        "psr-4": {
            "Gendiff\\": "src/"
        },
        "files": [
            "src/differ.php",
            "src/parse.php",
            "src/render.php",
            "src/utils.php",
            "src/renderers/pretty.php",
            "src/renderers/plain.php"
        ]
    },
    "require": {
        "docopt/docopt": "^1.0",
        "funct/funct": "*",
        "symfony/yaml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "squizlabs/php_codesniffer": "*",
        "codeclimate/php-test-reporter": "dev-master"
    }
}
